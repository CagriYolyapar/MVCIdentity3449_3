@model List<AppRole>

    <a asp-action="Create">Rol Ekle</a>

    <table class="table table-hover">
        <thead>
            <tr>
                <th>Id</th>
                <th>Rol ismi</th>
            </tr>
        </thead>

        <tbody>
        @foreach(AppRole role in Model)
        {
            <tr>
                <td>@role.Id</td>
                <td>@role.Name</td>
            </tr>
        }
        </tbody>

    </table>